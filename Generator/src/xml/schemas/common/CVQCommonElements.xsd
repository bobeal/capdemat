    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Validated" />
      <xs:enumeration value="Closed" />
      <xs:enumeration value="Archived" />
      <xs:enumeration value="Rejected" />
      <xs:enumeration value="Uncomplete" />
      <xs:enumeration value="Complete" />
      <xs:enumeration value="Cancelled" />
      <xs:enumeration value="Pending" />
      <xs:enumeration value="Notified" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DataStateType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Statut associé aux données d'une demande
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Statut des données
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/translation/enum">
        <enum key="Pending" value="En attente" />
        <enum key="Valid" value="Valide" />
        <enum key="Invalid" value="Invalide" />
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Pending" />
      <xs:enumeration value="Valid" />
      <xs:enumeration value="Invalid" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="SubjectType">
    <xs:choice>
      <xs:element name="Individual" type="cvq:IndividualType" />
      <xs:element name="Adult" type="cvq:AdultType" />
      <xs:element name="Child" type="cvq:ChildType" />
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="RequestType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Informations générales relatives à une demande administrative
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Demande
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RequestTypeLabel" type="xs:string" />
      <xs:element name="Id" type="xs:long">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Identifiant de la demande
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Identifiant
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Subject" type="cvq:SubjectType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Sujet d'une demande
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Sujet
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Requester" type="cvq:AdultType" />
      <xs:element name="CreationDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de création de la demande
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Date de création
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValidationDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de validation de la demande
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Date de validation
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LastModificationDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de dernière modification de la demande
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Dernière modification
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LastInterveningUserId" type="xs:long" />
      <xs:element name="Step">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
              Etape associée à la demande
            </xs:documentation>
            <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
              Etape
            </xs:documentation>
            <xs:documentation xml:lang="fr"
              source="http://www.cg95.fr/cvq/schema/translation/enum">
              <enum key="Instruction" value="Instruction" />
              <enum key="Delivery" value="Délivrance" />
            </xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Instruction" />
            <xs:enumeration value="Delivery" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="State" type="cvq:RequestStateType" />
      <xs:element name="DataState" type="cvq:DataStateType" />
      <xs:element name="HomeFolder" type="cvq:HomeFolderType" />
      <xs:element name="MeansOfContact" type="cvq:MeansOfContactType" />
      <xs:element name="RequestSeason" type="cvq:RequestSeasonType" minOccurs="0"/>
      <xs:element name="Observations" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="200" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <!-- allow additional information from referential (only) to be added to the request -->
      <xs:element name="AdditionalInformation" type="cvq:AdditionalInformationType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="RequestSeasonType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Saison de validité d'une demande
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Saison
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:long">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Identifiant de la saison
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Identifiant
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Label" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Libellé de la saison
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Libellé
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RegistrationStart" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de début des inscriptions
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Début des inscriptions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RegistrationEnd" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de fin des inscriptions
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Fin des inscriptions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValidationAuthorizationStart" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de début de validation des inscriptions
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Début de validation des inscriptions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EffectStart" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de début de la saison
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Début de la saison
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EffectEnd" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Date de fin de la saison
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Fin de la saison
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AdditionalInformationType">
    <xs:sequence>
      <xs:any namespace="http://www.cg95.fr/cvq/schema/common" minOccurs="0" maxOccurs="unbounded"
        processContents="lax" />
    </xs:sequence>
  </xs:complexType>

  <!-- LOCAL REFERENTIAL RELATED TYPES -->

  <xs:complexType name="LocalReferentialDataType">
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" type="xs:string" />
      <xs:element name="Priority" type="xs:positiveInteger" minOccurs="0" />
      <xs:element name="AdditionalInformationLabel" type="xs:string" minOccurs="0" />
      <xs:element name="AdditionalInformationValue" type="xs:string" minOccurs="0" />
      <xs:element name="Children" type="cvq:LocalReferentialDataType" minOccurs="0"
        maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="TicketTypeSelectionType">
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" type="xs:string" />
      <xs:element name="Number" type="xs:long" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PlaceReservationDataType">
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" type="xs:string" />
      <xs:element name="TicketTypeSelection" type="cvq:TicketTypeSelectionType"
        maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

</xs:schema>
