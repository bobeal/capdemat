<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.cg95.fr/cvq/schema/common"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cvq="http://www.cg95.fr/cvq/schema/common"
  elementFormDefault="qualified" attributeFormDefault="unqualified">

  <!-- REFERENTIAL/AUTHORITY RELATED TYPES -->

  <xs:complexType name="LocalAuthorityType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Identification d'une collectivité territoriale
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Collectivité Territoriale
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" type="xs:string" />
      <xs:element name="PostalCode" type="cvq:PostalCodeType" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SchoolType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Identification d'une école
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Ecole
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" default="">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
              Nom de l'école
            </xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]?.*" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Type">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
              Type de l'école (maternelle ou élémentaire)
            </xs:documentation>
            <xs:documentation xml:lang="fr"
              source="http://www.cg95.fr/cvq/schema/translation/enum">
              <enum key="Nursery" value="Maternelle" />
              <enum key="Primary" value="Primaire" />
            </xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Nursery" />
            <xs:enumeration value="Primary" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="RecreationCenterType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Identification d'un centre de loisirs
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Centre de loisirs
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Identification of a recreation center
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Recreation center
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Id" type="xs:long" />
      <xs:element name="Name" type="xs:string" />
      <xs:element name="Description" type="xs:string" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="SectionType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Informations caractérisant une classe - Le nom de la classe est composé du niveau et d'un
        élément (en général le nom de l'instituteur) permettant de distinguer les classes entre
        elles.
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Classe
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/translation/enum">
        <enum key="BeforeFirstSection" value="Très Petite Section" />
        <enum key="FirstSection" value="Petite Section" />
        <enum key="SecondSection" value="Moyenne Section" />
        <enum key="ThirdSection" value="Grande Section" />
        <enum key="CP" value="CP" />
        <enum key="CE1" value="CE1" />
        <enum key="CE2" value="CE2" />
        <enum key="CM1" value="CM1" />
        <enum key="CM2" value="CM2" />
        <enum key="CLISS" value="CLISS" />
        <enum key="Unknown" value="Inconnue" />
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="32" />
      <xs:enumeration value="BeforeFirstSection" />
      <xs:enumeration value="FirstSection" />
      <xs:enumeration value="SecondSection" />
      <xs:enumeration value="ThirdSection" />
      <xs:enumeration value="CP" />
      <xs:enumeration value="CE1" />
      <xs:enumeration value="CE2" />
      <xs:enumeration value="CM1" />
      <xs:enumeration value="CM2" />
      <xs:enumeration value="CLISS" />
      <xs:enumeration value="Unknown" />
    </xs:restriction>
  </xs:simpleType>

