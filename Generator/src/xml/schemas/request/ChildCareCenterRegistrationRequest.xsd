						<common>
							<step name="registrationParams"/>
							<conditions>
								<condition name="isMondayPeriodeChoice" type="trigger"/>
							</conditions>
						</common>
						<fo>
							<radio />
						</fo>
						<bo column="2"/>
						<pdf />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<!-- Begining first periode -->
			<xs:element name="MondayFirstPeriodBegining" type="xs:string" minOccurs="0">
				<xs:annotation>
		      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Début première période
		      </xs:documentation>
		      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Begining period
		      </xs:documentation>
					<xs:appinfo>
						<common>
							<step name="registrationParams"/>
						</common>
						<fo/>
						<bo column="2"/>
						<pdf />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<!-- Ending first periode -->
			<xs:element name="MondayFirstPeriodEnding" type="xs:string" minOccurs="0">
				<xs:annotation>
		      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Fin première période
		      </xs:documentation>
		      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Ending period
		      </xs:documentation>
					<xs:appinfo>
						<common>
							<step name="registrationParams"/>
						</common>
						<fo/>
						<bo column="2"/>
						<pdf />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<!-- Begining second periode -->
			<xs:element name="MondaySecondPeriodBegining" type="xs:string" minOccurs="0">
				<xs:annotation>
		      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Début seconde période
		      </xs:documentation>
		      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Begining period
		      </xs:documentation>
					<xs:appinfo>
						<common>
							<step name="registrationParams"/>
							<conditions>
								<condition name="isMondayPeriodeChoice" type="filled"/>
							</conditions>
						</common>
						<fo/>
						<bo column="2"/>
						<pdf />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<!-- Ending second periode -->
			<xs:element name="MondaySecondPeriodEnding" type="xs:string" minOccurs="0">
				<xs:annotation>
		      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Fin seconde période
		      </xs:documentation>
		      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
		        Ending period
		      </xs:documentation>
					<xs:appinfo>
						<common>
							<step name="registrationParams"/>
							<conditions>
							<condition name="isMondayPeriodeChoice" type="filled"/>
							</conditions>
						</common>
						<fo/>
						<bo column="2"/>
						<pdf />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
		</xs:sequence>
	</xs:complexType>	

